# OpenAI API Key (optional, for LLM-based query parsing)
OPENAI_API_KEY=your-openai-api-key-here

# NetBox Configuration (optional, falls back to sample data if not provided)
NETBOX_URL=https://netbox.example.com
NETBOX_TOKEN=your-netbox-api-token-here

# SSH Credentials (optional, for device identity verification)
SSH_USER=admin
SSH_PASS=your-ssh-password

# Telnet Credentials (optional, for device identity verification)
TELNET_USER=admin
TELNET_PASS=your-telnet-password

# Debug Mode (optional)
DEBUG=false

# MCP Server Configuration (if needed)
# MCP_SERVER_URL=...
# MCP_SERVER_TOKEN=...
